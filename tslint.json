{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "no-any": true,
        "no-var-requires": true,
        "no-bitwise": true,
        "no-duplicate-variable": true,
        "no-eval": true,
        "no-unused-variable": true,
        "no-var-keyword": true,
        "no-console": false
    },
    "rulesDirectory": []
}